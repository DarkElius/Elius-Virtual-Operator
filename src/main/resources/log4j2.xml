<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" name="virtualoperator" packages="">
    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5p %c{1} - %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="TRACE">
            <AppenderRef ref="stdout"/>
        </Root>
    </Loggers>
</Configuration>